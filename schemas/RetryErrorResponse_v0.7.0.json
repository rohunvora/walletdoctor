{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://walletdoctor.app/schemas/0.7.0/RetryErrorResponse.json",
  "title": "RetryErrorResponse",
  "description": "RetryErrorResponse schema",
  "type": "object",
  "required": [
    "error",
    "message",
    "retry_after"
  ],
  "properties": {
    "error": {
      "type": "string",
      "description": "Error type"
    },
    "message": {
      "type": "string",
      "description": "Error message"
    },
    "code": {
      "type": "string",
      "description": "Error code"
    },
    "retry_after": {
      "type": "integer",
      "description": "Seconds to wait before retry"
    }
  }
}